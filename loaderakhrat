local allowedUsers = {
    ["9wenerd"] = true,
    ["6atotkaca9"] = true,
    ["ShedowBlack7"] = true,
    ["AsepSlebewNie"] = true, 
}

local player = game.Players.LocalPlayer
if not allowedUsers[player.Name] then
    player:Kick("Access Denied: Kamu tidak punya izin menjalankan script ini.")
    return
end

-- âœ… Kalau lolos lock â†’ jalankan script utama
loadstring(game:HttpGet("https://raw.githubusercontent.com/aditpacked/pribaditoir/refs/heads/main/anjehehajhirat"))()